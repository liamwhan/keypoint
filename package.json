{
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "electron": "^20.0.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  },
  "name": "keypoint",
  "version": "0.0.1",
  "main": "dist/Main.js",
  "scripts": {
    "start": "set NODE_ENV= && electron-forge start -- --log-level=3" ,
    "watch": "tsc -w",
    "package": "electron-forge package",
    "make": "electron-forge make --arch=x64 --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamwhan/keypoint.git"
  },
  "author": "Liam Whan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/liamwhan/keypoint/issues"
  },
  "homepage": "https://github.com/liamwhan/keypoint#readme",
  "description": "",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "icon": "./misc/keypoint.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "keypoint",
            "description": "The simplest, fastest Slideshow and presentation application around"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
